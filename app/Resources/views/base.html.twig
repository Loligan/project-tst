<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        {% block head %} {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
        <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
        <script src="{{ asset('semantic-ui/semantic.min.js') }}"></script>
        <link rel="stylesheet" type="text/css" href="{{ asset('semantic-ui/semantic.min.css') }}">
    </head>
    <body>
    <nav class="ui mini menu">
        <a class="active item" href="/">
            Home
        </a>
        <a class="item" href="{{ path('category_index') }}">
            Category
        </a>
        <a class="item" href="{{ path('item_index') }}">
            Items
        </a>
        <a class="item" href="{{ path('img_index') }}">
            Images
        </a>
        <a class="item" href="{{ path('param_index') }}">
            Params
        </a>
        <div class="right menu">
            <div class="item">
                <div class="ui primary button">Sign Up</div>
            </div>
        </div>
    </nav>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
